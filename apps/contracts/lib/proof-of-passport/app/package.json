    "@sentry/react": "^9.32.0",
    "@sentry/react-native": "7.0.1",
    "@tamagui/animations-css": "^1.129.3",
    "@tamagui/animations-react-native": "^1.129.3",
    "@tamagui/config": "1.126.14",
